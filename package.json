{
  "name": "dear-amanda-app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "@headlessui/react": "^2.2.8",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.2.2",
    "@supabase/supabase-js": "^2.75.0",
    "@tanstack/react-query": "^5.90.2",
    "@tanstack/react-router": "^1.44.2",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "clsx": "^2.1.1",
    "postcss-nesting": "^13.0.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.65.0",
    "supabase": "^2.40.7",
    "tailwind": "^4.0.0",
    "tailwindcss": "^3.4.18",
    "typescript": "^5.9.2",
    "zod": "^4.1.12"
  },
  "main": "electron/main.cjs",
  "scripts": {
    "dev": "vite",
    "dev:staging": "vite --mode staging",
    "electron": "electron .",
    "electron:prod": "npm run build && electron .",
    "electron:dev": "concurrently \"cross-env NODE_ENV=development ELECTRON=true vite\" \"wait-on http://localhost:3000 && cross-env NODE_ENV=development electron .\"",
    "electron:build": "npm run build && electron-builder",
    "build": "tsc && vite build",
    "build:staging": "tsc && vite build --mode staging",
    "build:prod": "tsc && vite build --mode production",
    "preview": "vite preview",
    "generate": "npx @tanstack/router-cli generate"
  },
  "devDependencies": {
    "@tanstack/router-plugin": "^1.44.3",
    "@types/node": "^20.19.11",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^39.0.0",
    "electron-builder": "^26.0.12",
    "postcss": "^8.5.6",
    "vite": "^7.1.2",
    "wait-on": "^9.0.1"
  }
}
